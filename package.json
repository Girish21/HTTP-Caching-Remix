{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "build": "npm run build:css && npm run build:remix",
    "dev": "remix run",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "postinstall": "patch-package && remix setup",
    "start": "remix-serve build",
    "watch": "concurrently \"npm:watch:css\" \"npm:dev\"",
    "watch:css": "TAILWIND_MODE=watch postcss styles --base styles --dir app/styles -w"
  },
  "dependencies": {
    "@girish_kumar/md-utils": "^1.0.11",
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.10.0",
    "@remix-run/react": "^0.18.2",
    "@remix-run/serve": "^0.18.2",
    "dotenv": "^10.0.0",
    "mdx-bundler": "^6.0.1",
    "patch-package": "^6.4.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "6.0.0-beta.0",
    "remark": "^14.0.1",
    "remark-autolink-headings": "^7.0.1",
    "remark-gfm": "^2.0.0",
    "remark-slug": "^7.0.0",
    "remix": "^0.18.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.18.2",
    "@tailwindcss/typography": "^0.4.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.4",
    "autoprefixer": "^10.3.4",
    "concurrently": "^6.2.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
